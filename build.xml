<?xml version="1.0" encoding="UTF-8"?>
<project name="SEPT" default="default" basedir=".">        
    <import file="nbproject/build-impl.xml"/>

    <taskdef name="bundleapp"
             classname="com.oracle.appbundler.AppBundlerTask"   
             classpath="lib/appbundler-1.0.jar" />

    <target name="bundle-Papoy">
        <bundleapp outputdirectory="dist"
            name="Papoy"
            displayname="Papoy"
            identifier="Papoy"
            shortversion="2.6"
            icon="dist/PapoyIcon.icns"
            mainclassname="Papoy">
            <classpath file="dist/Papoy.jar" />
        </bundleapp>
    </target>

</project>